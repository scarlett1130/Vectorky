/* background mearts-overlay 
===================================*/
.mearts-overlay {
  position: relative;
}

.mearts-overlay:before {
  position: relative;
  top: 0;
  left: 0;
  width: 100%;
  height: 100%;
  content: '';
  background: rgba(0, 0, 0, 0.6);
  -webkit-transition: all 0.3s ease 0s;
  -o-transition: all 0.3s ease 0s;
  transition: all 0.3s ease 0s;
}
.mearts-overlay .container {
  position: relative;
  z-index: 2;
}
/* dark mearts-overlay 
================================*/
.mearts-overlay.dark-1:before {
  background-color: rgba(20, 20, 20, 0.85);
}
.mearts-overlay.dark-2:before {
  background-color: rgba(40, 40, 40, 0.9);
}
.mearts-overlay.dark-3:before {
  background-color: rgba(29, 29, 29, 0.8);
}
.mearts-overlay.dark-4:before {
  background-color: rgba(0, 0, 0, 0.6);
}
.mearts-overlay.dark-5:before {
  background-color: rgba(0, 0, 0, 0.9);
}

/* light mearts-overlay 
================================*/
.mearts-overlay.light-1:before {
  background-color: rgba(255, 255, 255, 0.5);
}

/* others color mearts-overlay 
=================================*/
.mearts-overlay-1:before {
  background-color: rgba(0, 54, 99, 0.9);
}
.mearts-overlay-2:before {
  background-color: rgba(0, 54, 99, 0.8);
}

