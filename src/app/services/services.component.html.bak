  <script>
      var jq=jQuery.noConflict();
  function sendContact() {
      var valid;	
      valid = validateContact();
      if(valid) {
          jQuery.ajax({
          url: "http://localhost:8080/vector/wp-content/themes/vector4/contact_mail.php",
          data:'userName='+jq(".fname").val()+'&userEmail='+jq(".femail").val()+'&phone='+jq(".fmobile").val()+'&content='+jq(".fmessage").val(),
          type: "POST",
          success:function(data){
          jq("#mail-status").html(data);
          },
          error:function (){}
          });
      }
  }
  
  function validateContact() {
      var valid = true;	
      /*jq(".demoInputBox").css('background-color','');
      jq(".info").html('');*/
      
      if(!jq(".fname").val()) {
          jq('.pfname').hide();
          jq('.pfnamereq').show();
          //jq("#userName-info").html("(required)");
          //jq("#userName").css('background-color','#FFFFDF');
          valid = false;
      }
      if(!jq(".fmobile").val()) {
          //jq("#userEmail-info").html("(required)");
          //jq("#userEmail").css('background-color','#FFFFDF');
          jq('.pmobile').hide();
          jq('.pmobilereq').show();
          valid = false;
      }
      
      if(!jq(".fmobile").val().match(/^\d*(?:\.\d{1,2})?$/)) {
          jq('.pmobile').hide();
          jq('.pmobilereq').show();
          //jq("#userEmail-info").html("(invalid)");
          //jq("#userEmail").css('background-color','#FFFFDF');
          valid = false;
      }
      
      if(!jq(".femail").val()) {
          //jq("#userEmail-info").html("(required)");
          //jq("#userEmail").css('background-color','#FFFFDF');
          jq('.pemail').hide();
          jq('.pemailreq').show();
          valid = false;
      }
      
      if(!jq(".femail").val().match(/^([\w-\.]+@([\w-]+\.)+[\w-]{2,4})?$/)) {
          jq('.pemail').hide();
          jq('.pemailreq').show();
          //jq("#userEmail-info").html("(invalid)");
          //jq("#userEmail").css('background-color','#FFFFDF');
          valid = false;
      }
      /*if(!jq("#subject").val()) {
          jq("#subject-info").html("(required)");
          jq("#subject").css('background-color','#FFFFDF');
          valid = false;
      }*/
      if(!jq(".fmessage").val()) {
          //jq("#content-info").html("(required)");
          //jq("#content").css('background-color','#FFFFDF');
          jq('.pmessage').hide();
          jq('.pmessagereq').show();
          valid = false;
      }
      
      return valid;
  }
  </script>					
                      
