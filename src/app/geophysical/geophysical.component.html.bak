      <div  class="wpb_single_image wpb_content_element vc_align_center   sv1mob">
          
          <figure class="wpb_wrapper vc_figure">
              <div class="vc_single_image-wrapper   vc_box_border_grey"><img width="555" height="507" src="assets/images/geophysical-icon.png" class="vc_single_image-img attachment-full" alt="" loading="lazy" srcset="assets/images/geophysical-icon.png 555w, assets/images/geophysical-icon-300x274.png 300w, assets/images/geophysical-icon-150x137.png 150w" sizes="(max-width: 555px) 100vw, 555px" /></div>
